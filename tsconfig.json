{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES6"
        ],
        "allowJs": true,
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}